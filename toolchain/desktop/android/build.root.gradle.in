// Top-level build file where you can add configuration options common to all sub-projects/modules.
buildscript {
    repositories {
        google()
        jcenter()
    }
    dependencies {
        classpath 'com.android.tools.build:gradle:@ANDROID_GRADLE_MODULE_VER@'
    }
}

allprojects {
    repositories {
        jcenter()
        google()
    }
}
