                "TOOLCHAIN_ROOT": "${sourceDir}/multi_build/compilers/gamecube-$env{ARCHITECTURE}/$env{TOOLCHAIN_VERSION}",
                "TOOLCHAIN_SYSROOT": "$env{TOOLCHAIN_ROOT}/$env{TOOLCHAIN_PREFIX}/sysroot",
